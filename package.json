{
  "name": "genericpage-editor",
  "version": "1.0.0",
  "description": "Generic-Page Editor",
  "private": true,
  "scripts": {
    "start": "cross-env umi dev ",
    "build": "umi build",
    "test": "umi test",
    "format": "yarn prettier && yarn lint --fix",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint": "eslint --ext .js src && npm run lint:style",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint:fix": "eslint --fix --ext .js src && npm run lint:style",
    "lint-staged:js": "eslint --ext .js",
    "precommit": "lint-staged",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "better-scroll": "^1.13.2",
    "blueimp-md5": "^2.10.0",
    "dva": "^2.4.0",
    "enzyme": "^3.4.4",
    "events": "^3.0.0",
    "hash.js": "^1.1.5",
    "lodash": "^4.17.11",
    "moment-mini": "^2.22.1",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.5.10",
    "qs": "^6.5.2",
    "rc-form": "^2.4.0",
    "react": "^16.5.1",
    "react-document-title": "^2.0.3"
  },
  "optionalDependencies": {
    "puppeteer": "^1.9.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "cross-env": "^5.1.1",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-umi": "^0.1.5",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^2.5.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "1.14.2",
    "stylelint": "^9.4.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.0.0",
    "umi": "^2.6.11",
    "umi-plugin-react": "^1.0.1",
    "vconsole": "^3.3.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
